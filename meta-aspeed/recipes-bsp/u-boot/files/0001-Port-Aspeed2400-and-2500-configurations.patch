---
 configs/ast_g4_ncsi_defconfig       | 11 +++++++++
 configs/ast_g4_phy_defconfig        | 14 ++++++++++++
 configs/ast_g5_ncsi_2boot_defconfig | 35 +++++++++++++++++++++++++++++
 configs/ast_g5_ncsi_defconfig       | 13 +++++++++++
 configs/ast_g5_phy_defconfig        | 14 ++++++++++++
 5 files changed, 87 insertions(+)
 create mode 100644 configs/ast_g4_ncsi_defconfig
 create mode 100644 configs/ast_g4_phy_defconfig
 create mode 100644 configs/ast_g5_ncsi_2boot_defconfig
 create mode 100644 configs/ast_g5_ncsi_defconfig
 create mode 100644 configs/ast_g5_phy_defconfig

diff --git a/configs/ast_g4_ncsi_defconfig b/configs/ast_g4_ncsi_defconfig
new file mode 100644
index 0000000000..a56401c765
--- /dev/null
+++ b/configs/ast_g4_ncsi_defconfig
@@ -0,0 +1,11 @@
+CONFIG_ARM=y
+CONFIG_SYS_TEXT_BASE=0x0
+CONFIG_ENV_SIZE=0x10000
+CONFIG_FIT=y
+CONFIG_FIT_VERBOSE=y
+CONFIG_HUSH_PARSER=y
+CONFIG_SYS_PROMPT="ast# "
+CONFIG_CMD_DHCP=y
+CONFIG_CMD_PING=y
+CONFIG_SYS_NS16550=y
+# CONFIG_EFI_LOADER is not set
diff --git a/configs/ast_g4_phy_defconfig b/configs/ast_g4_phy_defconfig
new file mode 100644
index 0000000000..f8db39ef13
--- /dev/null
+++ b/configs/ast_g4_phy_defconfig
@@ -0,0 +1,14 @@
+CONFIG_ARM=y
+CONFIG_TARGET_AST_G4=y
+CONFIG_ASPEED_NET_PHY=y
+CONFIG_SYS_PROMPT="ast# "
+CONFIG_CMD_BOOTEFI=n
+CONFIG_CMD_DHCP=y
+CONFIG_CMD_PING=y
+CONFIG_EFI_LOADER=n
+CONFIG_FIT=y
+CONFIG_FIT_VERBOSE=y
+CONFIG_HUSH_PARSER=y
+CONFIG_OF_LIBFDT=y
+CONFIG_SPI_FLASH=y
+CONFIG_SYS_NS16550=y
